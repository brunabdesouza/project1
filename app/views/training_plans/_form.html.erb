<%= form_with model: @training_plan, local: true do |f| %>

 <%= f.label :title %>
 <%= f.text_field :title %>
 <br>

 <%= f.label :goal %>
 <%= f.text_field :goal %>
 <br>

 <%= f.label :workouts %>
 <%= f.collection_check_boxes :workout_ids, @workout_templates, :id, :workout_type, include_hidden: false %>
 <br>

 <%= f.submit %>

 <% end %>
