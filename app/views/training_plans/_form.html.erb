<%= form_with model: @training_plan, local: true do |f| %>

 <%= f.label :title %>
 <%= f.text_field :title %>
 <br>

 <%= f.label :goal %>
 <%= f.text_field :goal %>
 <br>

 <%= f.label :workouts %>
 <%= f.collection_check_boxes :workout_ids, Workout.all, :workout_type, :workout_type %>
 <br>

 <%= f.submit %>

 <% end %>
