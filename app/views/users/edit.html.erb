<h2>Edit your details</h2>

<%= form_with model: @user, local: true, html: { multipart: true } do |f| %>

<%= f.label :name%>
<%= f.text_field :name %>
<br>

<%= f.label :email%>
<%= f.text_field :email %>
<br>

<%= f.label :password%>
<%= f.password_field :password %>
<br>

<%= f.label :image%>
<%= f.file_field :image %>
<br>

<%= f.submit 'Update' %>

<% end %>
